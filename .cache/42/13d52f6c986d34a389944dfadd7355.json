  newX = e.changedTouches[0].pageX\r\n        if (newX - initX > 50) {\r\n          eventHub['swipRight'].forEach\r\n            (fn => fn.bind(root)());\r\n          document.getElementById(2).classList.remove('current');\r\n          document.getElementById(1).classList.add('current')\r\n        } else if (initX - newX > 50) {\r\n          eventHub['swipLeft'].forEach(fn => fn.bind(root)());\r\n          document.getElementById(1).classList.remove('current');\r\n          document.getElementById(2).classList.add('current')\r\n        }\r\n      }, 100)\r\n    }\r\n\r\n    this.on = function (type, fn) {\r\n      if (eventHub[type]) {\r\n        eventHub[type].push(fn)\r\n      }\r\n    }\r\n\r\n    this.off = function (type, fn) {\r\n      let index = eventHub[type].indexOf(fn)\r\n      if (index !== -1) {\r\n        eventHub[type].splice(index, 1)\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n\r\nexport default Swiper"},"lineCount":null}},"error":null,"hash":"35fa36c329ab178e792a3fe2f63a1624","cacheData":{"env":{}}}